{
  "id": "deepseek-provider",
  "name": "DeepSeek Provider",
  "description": "DeepSeek (OpenAI-compatible) provider auth + config helper.",
  "providers": ["deepseek"],
  "version": "0.1.0",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "baseUrl": {
        "type": "string",
        "default": "https://api.deepseek.com/v1"
      },
      "api": {
        "type": "string",
        "default": "openai-completions"
      },
      "models": {
        "type": "array",
        "items": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "id": { "type": "string" },
            "name": { "type": "string" },
            "contextWindow": { "type": "number" },
            "maxOutput": { "type": "number" }
          },
          "required": ["id"]
        }
      },
      "defaultModel": { "type": "string" }
    }
  }
}
